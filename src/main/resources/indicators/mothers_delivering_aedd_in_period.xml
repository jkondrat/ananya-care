<indicator additive="false">
    <name>Number of mothers with either AEDD in a period</name>
    <categories>
        <category name="Birth preparedness plan"/>
    </categories>
    <owners>
        <user login="mrcaretest@gmail.com"/>
        <reportviews>
            <reportview name="Operational Manager"/>
        </reportviews>
    </owners>
    <trend>3</trend>
    <reports>
        <report type="Line Chart" label-x="Time" label-y="Value" />
    </reports>
    <area level="Block"/>
    <default-frequency>Daily</default-frequency>
    <numerator>
        <dw-query>
            <select-columns>
                <select-column aggregation="Count" fieldName="*" />
            </select-columns>
            <dimension tableName="mother_case" />
            <where-group>
                <where-group>
                    <where-condition type="period">
                        <table-name>mother_case</table-name>
                        <field>add</field>
                        <offset>9</offset>
                    </where-condition>
                </where-group>
                <where-group join="OR">
                    <where-condition type="period">
                        <table-name>mother_case</table-name>
                        <field>edd</field>
                        <offset>9</offset>
                    </where-condition>
                </where-group>
            </where-group>
        </dw-query>
    </numerator>
</indicator>
