<indicator additive="true">
    <name>test-indicator-using-computed-field</name>
    <classifications>
        <classification name="IFA Tablets" />
    </classifications>
    <owners>
        <reportviews>
            <reportview name="Program Manager"/>
        </reportviews>
    </owners>
    <trend>3</trend>
    <reports>
        <report type="Line Chart" label-x="Time" label-y="Mothers count" />
    </reports>
    <area level="State"/>
    <default-frequency>Daily</default-frequency>
    <numerator>
        <dw-query>
            <select-columns>
                <select-column aggregation="Count" fieldName="*" />
            </select-columns>
            <dimension tableName="mother_case" />
            <combineWith type="Join" dimension-key="id" foreign-key="case_id">
                <dw-query>
                    <select-columns>
                        <select-column tableName="bp_form" fieldName="case_id" />
                    </select-columns>
                    <dimension tableName="bp_form" />
                    <where-group>
                        <where-condition type="valueComparison">
                            <table-name>bp_form</table-name>
                            <field>test-computed-field</field>
                            <operator>&gt;=</operator>
                            <value>90</value>
                        </where-condition>
                    </where-group>
                </dw-query>
            </combineWith>
            <where-group>
                <where-condition type="period">
                    <table-name>mother_case</table-name>
                    <field>add</field>
                </where-condition>
            </where-group>
        </dw-query>
    </numerator>
</indicator>
