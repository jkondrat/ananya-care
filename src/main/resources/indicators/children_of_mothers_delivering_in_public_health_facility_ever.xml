<indicator additive="false">
    <name>Number of children delivered in a public health facility, ever</name>
    <categories>
        <category name="Birth preparedness plan"/>
    </categories>
    <owners>
        <user login="mrcaretest@gmail.com"/>
        <reportviews>
            <reportview name="Operational Manager"/>
        </reportviews>
    </owners>
    <trend>3</trend>
    <reports>
        <report type="Line Chart" label-x="Time" label-y="Value" />
    </reports>
    <area level="District"/>
    <default-frequency>Daily</default-frequency>
    <numerator>
        <dw-query>
            <select-columns>
                <select-column aggregation="Count" fieldName="*" />
            </select-columns>
            <dimension tableName="child_case" />
            <combineWith type="Join" dimension-key="mother_id" foreign-key="id">
                <dw-query>
                    <select-columns>
                        <select-column tableName="mother_case" fieldName="id" />
                    </select-columns>
                    <dimension tableName="mother_case" />
                    <combineWith type="Join" dimension-key="id" foreign-key="case_id">
                        <dw-query>
                            <select-columns>
                                <select-column tableName="registration_mother_form" fieldName="case_id" />
                            </select-columns>
                            <dimension tableName="registration_mother_form" />
                            <combineWith type="Union" dimension-key="id" foreign-key="case_id">
                                <dw-query>
                                    <select-columns>
                                        <select-column tableName="delivery_mother_form" fieldName="case_id" />
                                    </select-columns>
                                    <dimension tableName="delivery_mother_form" />
                                    <where-group>
                                        <where-condition type="valueComparison">
                                            <table-name>delivery_mother_form</table-name>
                                            <field>where_born</field>
                                            <operator>=</operator>
                                            <value>hospital</value>
                                        </where-condition>
                                        <where-condition type="calculationEndDate">
                                            <table-name>delivery_mother_form</table-name>
                                            <field>time_end</field>
                                        </where-condition>
                                        <where-group>
                                            <where-condition type="valueComparison">
                                                <table-name>delivery_mother_form</table-name>
                                                <field>which_hospital</field>
                                                <operator>=</operator>
                                                <value>block_phc</value>
                                            </where-condition>
                                        </where-group>
                                        <where-group join="OR">
                                            <where-condition type="valueComparison">
                                                <table-name>delivery_mother_form</table-name>
                                                <field>which_hospital</field>
                                                <operator>=</operator>
                                                <value>district_hospital</value>
                                            </where-condition>
                                        </where-group>
                                    </where-group>
                                </dw-query>
                            </combineWith>
                            <where-group>
                                <where-condition type="valueComparison">
                                    <table-name>registration_mother_form</table-name>
                                    <field>where_born</field>
                                    <operator>=</operator>
                                    <value>hospital</value>
                                </where-condition>
                                <where-condition type="calculationEndDate">
                                    <table-name>registration_mother_form</table-name>
                                    <field>time_end</field>
                                </where-condition>
                                <where-group>
                                    <where-condition type="valueComparison">
                                        <table-name>registration_mother_form</table-name>
                                        <field>which_hospital</field>
                                        <operator>=</operator>
                                        <value>block_phc</value>
                                    </where-condition>
                                </where-group>
                                <where-group join="OR">
                                    <where-condition type="valueComparison">
                                        <table-name>registration_mother_form</table-name>
                                        <field>which_hospital</field>
                                        <operator>=</operator>
                                        <value>district_hospital</value>
                                    </where-condition>
                                </where-group>
                            </where-group>
                        </dw-query>
                    </combineWith>
                </dw-query>
            </combineWith>
        </dw-query>
    </numerator>
</indicator>
