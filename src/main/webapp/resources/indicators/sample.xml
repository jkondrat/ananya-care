<indicator>
    <name>Some indicator</name>
    <categories>
        <category name="IFA Tablets" />
    </categories>
    <users>
        <user login="userName" />
    </users>
    <roles>
        <role name="roleName" />
    </roles>
    <trend>3</trend>
    <area name="Bihar" level="State" />
    <default-frequency>Daily</default-frequency>
    <denominator>
        <dw-query>
            <dimension tableName="can be every table" />
            <facts>
                <fact>
                    <name>(can be every table that has foreign-key to dimension)</name>
                    <where-group>
                        <where-condition type="valueComparison">
                            <field>ifa_tablets_total</field>
                            <operator>=</operator>
                            <value>90</value>
                        </where-condition>
                    </where-group>
                    <groupedByDimensionFactRowsNumber compSymbol=">=" value="2" />
                </fact>
            </facts>
            <where-group>
                <group join="and">
                    <where-condition type="valueComparison">
                        <field>ifa_tablets_total</field>
                        <operator>=</operator>
                        <value>90</value>
                    </where-condition>
                </group>
                <group join="or">
                    <where-condition type="valueComparison">
                        <field>ifa_tablets_total</field>
                        <operator>=</operator>
                        <value>90</value>
                    </where-condition>
                </group>
            </where-group>
        </dw-query>
    </denominator>
    <union>
        <union-elements>
            <union-element>
                <indicator id="2" />
            </union-element>
        </union-elements>
    </union>
</indicator>