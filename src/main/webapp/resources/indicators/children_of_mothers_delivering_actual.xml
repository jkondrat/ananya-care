<indicator additive="true">
    <name>Number of children delivered in the period</name>
    <categories>
        <category name="Birth preparedness plan"/>
    </categories>
    <owners>
        <user login="mrcaretest@gmail.com"/>
        <reportviews>
            <reportview name="Manager"/>
        </reportviews>
    </owners>
    <trend>3</trend>
    <reports>
        <report type="Line Chart" label-x="Time" label-y="Value" />
    </reports>
    <area name="Bihar" level="State"/>
    <default-frequency>Daily</default-frequency>
    <numerator>
        <dw-query dimension-key="mother_id" fact-key="id">
            <select-columns>
                <select-column aggregation="Count" fieldName="*" />
            </select-columns>
            <dimension tableName="child_case" />
            <facts>
                <fact>
                    <name>mother_case</name>
                    <select-columns>
                        <select-column tableName="mother_case" fieldName="*" />
                    </select-columns>
                </fact>
            </facts>
            <where-group>
                <where-condition type="period">
                    <table-name>facts</table-name>
                    <field>add</field>
                </where-condition>
            </where-group>
        </dw-query>
    </numerator>
</indicator>
